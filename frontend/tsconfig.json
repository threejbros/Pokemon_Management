{
    "compilerOptions": {
      "target": "ES6",
      "module": "ES6",
      "lib": ["DOM", "ES6"],
      "allowJs": true,
      "jsx": "react",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "resolveJsonModule": true,
      "moduleResolution": "node",
      "isolatedModules": true,
      "noEmit": true
    },
    "include": ["src"],
    "exclude": ["node_modules", "dist"],
}